---
import PageHeader from "../../components/PageHeader.astro";
import * as agreement from "../../data/agreement-en.md";
import Layout from "../../layouts/Layout.astro";
const title = "NocoBase License Agreement";
---

<Layout title={title}>
  <PageHeader title={title} />

  <!-- Start Agreement -->
  <section class="section">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-9">
          <div class="card shadow border-0 rounded">
            <div class="card-body license-body">{agreement.rawContent()}</div>
          </div>
        </div><!--end col-->
      </div><!--end row-->
    </div><!--end container-->
  </section><!--end section-->
  <!-- End agreement -->
</Layout>
